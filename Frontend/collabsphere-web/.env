# API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_WS_URL=ws://localhost:5000

# WebRTC Signaling
REACT_APP_SIGNALING_URL=http://localhost:5002

# Whiteboard
REACT_APP_WHITEBOARD_WS=ws://localhost:5000/whiteboard

# Cloudinary for file uploads
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloud-name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your-upload-preset
REACT_APP_CLOUDINARY_API_KEY=your-api-key

# JWT
REACT_APP_JWT_SECRET=your-jwt-secret

# Meeting Settings
REACT_APP_MEETING_TIMEOUT=3600000
REACT_APP_MAX_PARTICIPANTS=50

# Feature Flags
REACT_APP_ENABLE_SCREEN_SHARING=true
REACT_APP_ENABLE_RECORDING=true
REACT_APP_ENABLE_CHAT=true