{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/subjects/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/subjects/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "subject"
    },
    {
      "DownstreamPathTemplate": "/api/classes/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5003
        }
      ],
      "UpstreamPathTemplate": "/classes/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "class"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "subject",
      "Config": [
        {
          "Name": "Subject Service",
          "Version": "v1",
          "Url": "https://localhost:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "class",
      "Config": [
        {
          "Name": "Class Service",
          "Version": "v1",
          "Url": "https://localhost:5003/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000"
  }
}
